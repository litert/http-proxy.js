    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/HttpProxy] Test HTTPS request over proxy",
            "program": "${workspaceFolder}/src/examples/test-https.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "http_proxy": "http://user:passwd@127.0.0.1:8888"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/HttpProxy] Test HTTP request over proxy",
            "program": "${workspaceFolder}/src/examples/test-http.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "http_proxy": "http://user:passwd@127.0.0.1:8888"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/HttpProxy] Test HTTP2 request over proxy",
            "program": "${workspaceFolder}/src/examples/test-http2.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "http_proxy": "http://user:passwd@127.0.0.1:8888"
            }
        }
    ]
}